@model IEnumerable<KutuphaneOtomasyonu.Controllers.AuditLogRow>
@{
    ViewData["Title"] = "Audit Log";
}

<h2>Audit Log</h2>
<hr />

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>İşlem</th>
            <th>Zaman</th>
            <th>LoanId</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var r in Model)
    {
        <tr>
            <td>@r.action</td>
            <td>@r.action_time</td>
            <td>@r.loan_id</td>
        </tr>
    }
    </tbody>
</table>

<a class="btn btn-secondary" asp-controller="Reports" asp-action="Index">Geri Dön</a>




